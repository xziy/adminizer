<%# detection type %>
<% var type = field.config.type || field.model.type %>
<% type = type.toLowerCase() %>
<% var isIn = typeof field.config.isIn === 'object' ? field.config.isIn : (field.model && typeof {...field.model.validations}.isIn === 'object' ? {...field.model.validations}.isIn : []) %>
<%# plain inputs %>
<%# Here applied select for string + isIn%>
<% var inputTextTypes = ['string', 'password', 'date', 'datetime', 'time', 'integer', 'number', 'float', 'color', 'email', 'month', 'week', 'range'] %>
<% if (inputTextTypes.includes(type)) { %>
    <%- include("fields/inputText", {type: type, isIn: isIn}) %>
<% } %>
<%# select %>
<% var selectTypes = ['select'] %>
<% if (selectTypes.includes(type)) { %>
    <%- include("fields/select") %>
<% } %>
<%# select-many %>
<% var selectManyTypes = ['select-many'] %>
<% if (selectManyTypes.includes(type)) { %>
    <%- include("fields/selectMany") %>
<% } %>
<%# boolean %>
<% if (type === 'boolean' || type === 'binary') { %>
    <%- include("fields/inputBoolean") %>
<% } %>
<%# textarea %>
<% var inputTextAreaTypes = ['text', 'longtext', 'mediumtext'] %>
<% if (inputTextAreaTypes.includes(type)) { %>
    <%- include("fields/inputTextarea") %>
<% } %>
<%# wysiwyg %>
<% var widgetWysiwygTypes = ['ckeditor', 'wysiwyg', 'texteditor', 'word'] %>
<% if (widgetWysiwygTypes.includes(type)) { %>
    <%- include("fields/widgetWysiwyg") %>
<% } %>
<%# tui editor %>
<% var widgetTuiEditorTypes = ['tui', 'tuieditor', 'toast-ui'] %>
<% if (widgetTuiEditorTypes.includes(type)) { %>
    <%- include("fields/widgetTuiEditor") %>
<% } %>
<%# jsoneditor %>
<% var widgetJsonEditorTypes = ['jsoneditor', 'json', 'array', 'object'] %>
<% if (widgetJsonEditorTypes.includes(type)) { %>
    <%- include("fields/widgetJsonEditor") %>
<% } %>
<%# ace %>
<% var widgetAceTypes = ['ace', 'html', 'xml', 'aceeditor'] %>
<% if (widgetAceTypes.includes(type)) { %>
    <%- include("fields/widgetAce") %>
<% } %>
<%# EditSchedule %>
<% var widgetEditSchedule = ['schedule', 'worktime'] %>
<% if (widgetEditSchedule.includes(type)) { %>
    <%- include("fields/widgetEditSchedule") %>
<% } %>
<%# GeoJsonEditor %>
<% var widgetGeoJsonEditor = ['geojson'] %>
<% if (widgetGeoJsonEditor.includes(type)) { %>
    <%- include("fields/widgetGeoJsonEditor") %>
<% } %>
<%# GeoJsonEditor %>
<% var widgetGeoJsonEditor = ['geo-polygon'] %>
<% if (widgetGeoJsonEditor.includes(type)) { %>
    <%- include("fields/widgetGeoPolygonEditor") %>
<% } %>
<%# GeoJsonEditor %>
<% var widgetGeoJsonEditor = ['geo-marker'] %>
<% if (widgetGeoJsonEditor.includes(type)) { %>
    <%- include("fields/widgetGeoMarkerEditor") %>
<% } %>
<%# Select-pure %>
<% if (type === 'association') { %>
    <%- include("fields/inputAssociation") %>
<% } %>
<%# Select-pure with multiple choice %>
<% if (type === 'association-many') { %>
    <%- include("fields/inputAssociationMany") %>
<% } %>
<%# Handsontable %>
<% if (type === 'table') { %>
    <%- include("fields/widgetHandsontable") %>
<% } %>
<%# Mediamanager %>
<% if (type === 'mediamanager') { %>
    <%- include("fields/mediamanager") %>
<% } %>
