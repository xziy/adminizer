<div id="<%= key %>-container" class="geoJsonEditor"></div>
<textarea
        <%= field.config.required && 'required' %>
        <%= field.config.disabled && 'disabled' %>
        name="<%= key %>"
        id="form-<%= key %>"
        rows="10" hidden><%- JSON.stringify(value) %></textarea>

<script>
  new GeoJsonEditor({
    id: '<%= key %>-container',
    field: '<%= key %>',
    data: '#form-<%= key %>',
    polygonEditor: true
  });
</script>
