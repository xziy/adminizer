<textarea
        <%= field.config.required && 'required' %>
        <%= field.config.disabled && 'disabled' %>
        name="<%= key %>"
        id="form-<%= key %>"
        rows="10" hidden><%- JSON.stringify(value) %></textarea>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        const options<%= key %> = <%- JSON.stringify(field.config.options) %>
            new EditSchedule({
                ...options<%= key %>,
                element: '<%= key %>-container',
                field: '<%= key %>',
                data: document.getElementById(`form-<%= key %>`).value
            });
    })
</script>
